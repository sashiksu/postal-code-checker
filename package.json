{
  "name": "postal-code-validation",
  "version": "1.0.0",
  "description": "Foobar is a Python library for dealing with word pluralization.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "start": "webpack serve --open --config webpack.config.js",
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\"",
    "prepare": "npm run build",
    "publish:check": "npm run prettier && npm run lint && npm publish --dry-run",
    "publish": "npm publish",
    "build-new": "webpack",
    "watch": "nodemon --watch src --ext ts,tsx --exec \"npm run build\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "antd": "^5.19.4",
    "babel-loader": "^9.1.3",
    "eslint": "^9.7.0",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^2.0.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
