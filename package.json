{
  "name": "postal-code-validation",
  "version": "1.0.0",
  "description": "Foobar is a Python library for dealing with word pluralization.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "start": "webpack serve --open",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\"",
    "prepare": "npm run build",
    "publish:check": "npm run prettier && npm run lint && npm publish --dry-run",
    "publish": "npm publish",
    "build-new": "webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "babel-loader": "^9.1.3",
    "eslint": "^9.7.0",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
